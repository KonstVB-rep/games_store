{
    "compilerOptions": {
        "checkJs": true,
        "jsx": "react",
        "baseUrl": "src",
        "paths": {
            "@src/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@pages/*": ["src/pages/*"],
            "@assets/*": ["src/assets/*"],
            "@contexts/*": ["src/contexts/*"]
        }
    },
    "include": ["src", "src/router", "src/components", "src/pages"]
}
